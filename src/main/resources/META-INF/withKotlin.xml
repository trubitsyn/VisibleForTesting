<idea-plugin url="http://github.com/trubitsyn/idea-visiblefortesting">
    <extensions defaultExtensionNs="com.intellij">
        <intentionAction>
            <className>io.github.trubitsyn.visiblefortesting.intention.AnnotateKtClassOrObjectMethodsIntention</className>
        </intentionAction>
        <intentionAction>
            <className>io.github.trubitsyn.visiblefortesting.intention.AnnotateKtFunctionIntention</className>
